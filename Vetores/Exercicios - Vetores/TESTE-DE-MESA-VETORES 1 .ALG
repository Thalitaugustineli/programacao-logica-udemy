Algoritmo "TESTE-DE-MESA-VETORES 1 "

Var
// Variaveis
   a, b, c : real
   v : vetor[1..6] de real

Inicio
// Informando as váriaveis:
   a <- 10
   b <- 20
   c <- (a + b) / 2
   c <- c- 40
   v[4] <- a + b + c
   
// Pedindo para imprimir:

    escreva("O valor de v[4] é: ", v[4])


Fimalgoritmo